{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  props: {\n    responseData: {\n      type: Object,\n      required: true\n    }\n  },\n  methods: {\n    saveToFirestore(response) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          // title: Test,\n          // subtitle: this.timerange.content,\n          content: response.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEC,MADM;MAEZC,QAAQ,EAAE;IAFE;EADT,CADM;EAObC,OAAO,EAAE;IACPC,eAAe,CAACC,QAAD,EAAW;MACxB,MAAMC,WAAU,GAAIb,IAAI,CAACa,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwB,KAAKC,MAA7B;QACAd,MAAM,CAACF,UAAU,CAACC,GAAG,CAACD,UAAU,CAACD,EAAD,EAAK,MAAL,CAAX,EAAyBa,MAAzB,CAAJ,EAAsC,OAAtC,CAAX,EAA2D;UAC/DK,GAAG,EAAEL,MAD0D;UAE/D;UACA;UACAM,OAAO,EAAER,QAAQ,CAACQ;QAJ6C,CAA3D,CAAN,CAMGC,IANH,CAMQ,MAAM;UACVL,OAAO,CAACC,GAAR,CAAY,0BAAZ;QACD,CARH,EASGK,KATH,CASUC,KAAD,IAAW;UAChBP,OAAO,CAACO,KAAR,CAAc,kCAAd,EAAkDA,KAAlD;QACD,CAXH;MAYF,CAfA,MAeO;QACLP,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACF;IACD;;EArBM;AAPI,CAAf","names":["auth","db","collection","doc","addDoc","props","responseData","type","Object","required","methods","saveToFirestore","response","currentUser","userId","uid","console","log","result","uId","content","then","catch","error"],"sourceRoot":"","sources":["/Users/jasonti/Project/Tripper/front/src/views/PlanView.vue"],"sourcesContent":["<template>\n  <div class=\"card-wrapper\">\n    <div class=\"card-container\">\n      <div class=\"card bg-dark text-white\">\n        <div class=\"card-body\">\n          <h5 class=\"card-title\">Test</h5>\n          <h6 class=\"card-subtitle mb-2\">Test</h6>\n          <p class=\"card-text\">{{ responseData.data }}</p>\n          <button @click=\"saveToFirestore(response)\">Save</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\n\nexport default {\n  props: {\n    responseData: {\n      type: Object,\n      required: true,\n    }\n  },\n  methods: {\n    saveToFirestore(response) {\n      const currentUser = auth.currentUser;\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          // title: Test,\n          // subtitle: this.timerange.content,\n          content: response.content,\n        })\n          .then(() => {\n            console.log(\"Card added to Firestore!\");\n          })\n          .catch((error) => {\n            console.error(\"Error adding card to Firestore: \", error);\n          });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.card-wrapper {\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n}\n\n.card-container {\n  margin-right: 20px;\n  max-width: 400px;\n}\n\n.card {\n  max-width: 400px;\n  height: 400px;\n}\n\n.card-title {\n  font-size: 1.5rem;\n  color: white;\n}\n\n.card-subtitle {\n  font-size: 1.2rem;\n  color: white;\n}\n\n.card-text {\n  font-size: 1.2rem;\n  max-height: 100px;\n  overflow: hidden;\n}\n</style>\n\n\n"]},"metadata":{},"sourceType":"module"}