{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  computed: {\n    responseData() {\n      const response = JSON.parse(this.$route.query.response);\n      const chatResponseIndex = response.indexOf(\"chat_response:\");\n\n      if (chatResponseIndex !== -1) {\n        return response.substring(chatResponseIndex + 15);\n      }\n\n      return response;\n    }\n\n  },\n  methods: {\n    saveToFirestore() {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          // title: Test,\n          // subtitle: this.timerange.content,\n          content: responseData\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,QAAQ,EAAE;IACRC,YAAY,GAAG;MACb,MAAMC,QAAO,GAAIC,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBJ,QAA7B,CAAjB;MACA,MAAMK,iBAAgB,GAAIL,QAAQ,CAACM,OAAT,CAAiB,gBAAjB,CAA1B;;MACA,IAAID,iBAAgB,KAAM,CAAC,CAA3B,EAA8B;QAC5B,OAAOL,QAAQ,CAACO,SAAT,CAAmBF,iBAAgB,GAAI,EAAvC,CAAP;MACF;;MACA,OAAOL,QAAP;IACD;;EARO,CADG;EAWbQ,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,MAAMC,WAAU,GAAIjB,IAAI,CAACiB,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwB,KAAKC,MAA7B;QACAlB,MAAM,CAACF,UAAU,CAACC,GAAG,CAACD,UAAU,CAACD,EAAD,EAAK,MAAL,CAAX,EAAyBiB,MAAzB,CAAJ,EAAsC,OAAtC,CAAX,EAA2D;UAC/DK,GAAG,EAAEL,MAD0D;UAE/D;UACA;UACAM,OAAO,EAAElB;QAJsD,CAA3D,CAAN,CAMGmB,IAN