{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  data() {\n    return {\n      response: null,\n      destination: null,\n      timerange: null\n    };\n  },\n\n  mounted() {\n    this.response = JSON.parse(this.$route.query.response);\n    this.destination = JSON.parse(this.$route.query.result).destination;\n    this.timerange = JSON.parse(this.$route.query.result).rangeTimePicker;\n  },\n\n  methods: {\n    saveToFirestore(response) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          title: this.destination.content,\n          subtitle: this.timerange.content,\n          content: response.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,WAAW,EAAE,IAFR;MAGLC,SAAS,EAAE;IAHN,CAAP;EAKD,CAPY;;EAQbC,OAAO,GAAG;IACR,KAAKH,QAAL,GAAgBI,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBP,QAA7B,CAAhB;IACA,KAAKC,WAAL,GAAmBG,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,MAA7B,EAAqCP,WAAxD;IACA,KAAKC,SAAL,GAAiBE,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,MAA7B,EAAqCC,eAAtD;EACD,CAZY;;EAcbC,OAAO,EAAE;IACPC,eAAe,CAACX,QAAD,EAAW;MACxB,MAAMY,WAAU,GAAIlB,IAAI,CAACkB,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwB,KAAKR,MAA7B;QACAV,MAAM,CAACF,UAAU,CAACC,GAAG,CAACD,UAA