{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection } from \"firebase/firestore\";\nexport default {\n  data() {\n    return {\n      cards: [{\n        title: \"Card 1\",\n        subtitle: \"Subtitle 1\",\n        content: \"Content 1\"\n      }, {\n        title: \"Card 2\",\n        subtitle: \"Subtitle 2\",\n        content: \"Content 2\"\n      }, {\n        title: \"Card 3\",\n        subtitle: \"Subtitle 3\",\n        content: \"Content 3\"\n      }]\n    };\n  },\n\n  methods: {\n    saveToFirestore(card) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        collection(db, \"users\").doc(userId).collection(\"cards\").add({\n          title: card.title,\n          subtitle: card.subtitle,\n          content: card.content,\n          timestamp: new Date()\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,QAA2B,oBAA3B;AAGA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CADK,EAML;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CANK,EAWL;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CAXK;IADF,CAAP;EAmBD,CArBY;;EAsBbC,OAAO,EAAE;IACPC,eAAe,CAACC,IAAD,EAAO;MACpB,MAAMC,WAAU,GAAIX,IAAI,CAACW,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAX,UAAU,CAACD,EAAD,EAAK,OAAL,CAAV,CAAwBa,GAAxB,CAA4BF,MAA5B,EAAoCV,UAApC,CAA+C,OAA/C,EAAwDa,GAAxD,CAA4D;UAC1DV,KAAK,EAAEK,IAAI,CAACL,KAD8C;UAE1DC,QAAQ,EAAEI,IAAI,CAACJ,QAF2C;UAG1DC,OAAO,EAAEG,IAAI,CAACH,OAH4C;UAI1DS,SAAS,EAAE,IAAIC,IAAJ;QAJ+C,CAA5D,EAMCC,IAND,CAMM,MAAM;UACVC,OAAO,CAACC,GAAR