{"ast":null,"code":"import { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"123456\"\n    };\n  },\n\n  methods: {\n    register(submitEvent) {\n      // data update\n      this.email = submitEvent.target.elements.email.value;\n      this.password = submitEvent.target.elements.password.value; // firebase registration\n\n      const auth = getAuth();\n      createUserWithEmailAndPassword(auth, this.email, this.password).then(userCredential => {\n        const user = userCredential.user;\n        console.log(user);\n        console.log(\"Registration completed\");\n        this.$router.push(\"/\");\n      }).catch(error => {\n        const errorCode = error.code;\n        const errorMessage = error.message;\n        console.log(errorCode);\n        console.log(errorMessage);\n        let alert_2 = document.querySelector(\"#alert_2\");\n        alert_2.classList.remove(\"d-none\");\n        alert_2.innerHTML = errorMessage;\n        console.log(alert_2);\n      });\n    },\n\n    moveToLogin() {\n      this.$router.push(\"/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDA,SAASA,OAAT,EAAkBC,8BAAlB,QAAwD,eAAxD;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,QAAQ,EAAE;IAFL,CAAP;EAID,CANY;;EAObC,OAAO,EAAE;IACPC,QAAQ,CAACC,WAAD,EAAc;MACpB;MACA,KAAKJ,KAAL,GAAaI,WAAW,CAACC,MAAZ,CAAmBC,QAAnB,CAA4BN,KAA5B,CAAkCO,KAA/C;MACA,KAAKN,QAAL,GAAgBG,WAAW,CAACC,MAAZ,CAAmBC,QAAnB,CAA4BL,QAA5B,CAAqCM,KAArD,CAHoB,CAKpB;;MACA,MAAMC,IAAG,GAAIX,OAAO,EAApB;MACAC,8BAA8B,CAACU,IAAD,EAAO,KAAKR,KAAZ,EAAmB,KAAKC,QAAxB,CAA9B,CACGQ,IADH,CACSC,cAAD,IAAoB;QACxB,MAAMC,IAAG,GAAID,cAAc,CAACC,IAA5B;QACAC,OAAO,CAACC,GAAR,CAAYF,IAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,wBAAZ;QACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,GAAlB;MACD,CANH,EAOGC,KAPH,CAOUC,KAAD,IAAW;QAChB,MAAMC,SAAQ,GAAID,KAAK,CAACE,IAAxB;QACA,MAAMC,YAAW,GAAIH,KAAK,CAACI,OAA3B;QACAT,OAAO,CAACC,GAAR,CAAYK,SAAZ;QACAN,OAAO,CAACC,GAAR,CAAYO,YAAZ;QACA,IAAIE,OAAM,GAAIC,QAAQ,CAACC,aAAT,CAAuB,UAAvB,CAAd;QACAF,OAAO,CAACG,SAAR,CAAkBC,MAAlB,CAAyB,QAAzB;QACAJ,OAAO,CAACK,SAAR,GAAoBP,YAApB;QACAR,OAAO,CAACC,GAAR,CAAYS,OAAZ;MACD,CAhBH;IAiBD,CAzBM;;IA0BPM,WAAW,GAAG;MACZ,KAAKd,OAAL,CAAaC,IAAb,CAAkB,GAAlB;IACD;;EA5BM;AAPI,CAAf","names":["getAuth","createUserWithEmailAndPassword","data","email","password","methods","register","submitEvent","target","elements","value","auth","then","userCredential","user","console","log","$router","push","catch","error","errorCode","code","errorMessage","message","alert_2","document","querySelector","classList","remove","innerHTML","moveToLogin"],"sourceRoot":"","sources":["/Users/zhen/Downloads/vue-firebase-auth-main/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <form @submit.prevent=\"register\">\n      <h2 class=\"mb-3\">Register</h2>\n      <div class=\"input\">\n        <label for=\"email\">Email address</label>\n        <input\n          class=\"form-control\"\n          type=\"text\"\n          name=\"email\"\n          placeholder=\"email@adress.com\"\n        />\n      </div>\n      <div class=\"input\">\n        <label for=\"password\">Password</label>\n        <input\n          class=\"form-control\"\n          type=\"password\"\n          name=\"password\"\n          placeholder=\"password123\"\n        />\n      </div>\n\n      <div class=\"alternative-option mt-4\">\n        Already have an account? <span @click=\"moveToLogin\">Login</span>\n      </div>\n\n      <button type=\"submit\" id=\"register_button\" class=\"mt-4 btn-pers\">\n        Register\n      </button>\n      <div\n        class=\"alert alert-warning alert-dismissible fade show mt-5 d-none\"\n        role=\"alert\"\n        id=\"alert_2\"\n      >\n        Lorem ipsum dolor sit amet consectetur, adipisicing elit.\n        <button\n          type=\"button\"\n          class=\"btn-close\"\n          data-bs-dismiss=\"alert\"\n          aria-label=\"Close\"\n        ></button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"123456\",\n    };\n  },\n  methods: {\n    register(submitEvent) {\n      // data update\n      this.email = submitEvent.target.elements.email.value;\n      this.password = submitEvent.target.elements.password.value;\n\n      // firebase registration\n      const auth = getAuth();\n      createUserWithEmailAndPassword(auth, this.email, this.password)\n        .then((userCredential) => {\n          const user = userCredential.user;\n          console.log(user);\n          console.log(\"Registration completed\");\n          this.$router.push(\"/\");\n        })\n        .catch((error) => {\n          const errorCode = error.code;\n          const errorMessage = error.message;\n          console.log(errorCode);\n          console.log(errorMessage);\n          let alert_2 = document.querySelector(\"#alert_2\");\n          alert_2.classList.remove(\"d-none\");\n          alert_2.innerHTML = errorMessage;\n          console.log(alert_2);\n        });\n    },\n    moveToLogin() {\n      this.$router.push(\"/\");\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}