{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  computed: {\n    responseData() {\n      return JSON.parse(this.$route.query.response);\n    }\n\n  },\n  methods: {\n    saveToFirestore() {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          // title: Test,\n          // subtitle: this.timerange.content,\n          content: this.responseData\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,QAAQ,EAAE;IACRC,YAAY,GAAG;MACb,OAAOC,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,QAA7B,CAAP;IACD;;EAHO,CADG;EAMbC,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,MAAMC,WAAU,GAAId,IAAI,CAACc,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwB,KAAKC,MAA7B;QACAf,MAAM,CAACF,UAAU,CAACC,GAAG,CAACD,UAAU,CAACD,EAAD,EAAK,MAAL,CAAX,EAAyBc,MAAzB,CAAJ,EAAsC,OAAtC,CAAX,EAA2D;UAC/DK,GAAG,EAAEL,MAD0D;UAE/D;UACA;UACAM,OAAO,EAAE,KAAKf;QAJiD,CAA3D,CAAN,CAMGgB,IANH,CAMQ,MAAM;UACVL,OAAO,CAACC,GAAR,CAAY,0BAAZ;QACD,CARH,EASGK,KATH,CASUC,KAAD,IAAW;UAChBP,OAAO,CAACO,KAAR,CAAc,kCAAd,EAAkDA,KAAlD;QACD,CAXH;MAYF,CAfA,MAeO;QACLP,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACF;IACD;;EArBM;AANI,CAAf","names":["auth","db","collection","doc","addDoc","computed","responseData","JSON","parse","$route","query","response","methods","saveToFirestore","currentUser","userId","uid","console","log","result","uId","content","then","catch","error"],"sourceRoot":"","sources":["/Users/jasonti/Project/Tripper/front/src/views/PlanView.vue"],"sourcesContent":["<template>\n  <div class=\"card-wrapper\">\n    <div class=\"card-container\">\n      <div class=\"card bg-dark text-white\">\n          <h5 class=\"card-title\">{{ responseData[\"form_data\"][\"destination\"] }}</h5>\n          <h6 class=\"card-subtitle mb-2\">{{ responseData[\"form_data\"][\"rangeTimePicker\"] }}</h6>\n          <p class=\"card-text scrollable\">{{ responseData[\"chat_response\"] }}</p>\n      </div>\n      <button @click=\"saveToFirestore(response)\">Save</button>\n    </div>\n  </div>\n</template>\n\n\n\n<script>\nimport { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\n\nexport default {\n  computed: {\n    responseData() {\n      return JSON.parse(this.$route.query.response);\n    },\n  },\n  methods: {\n    saveToFirestore() {\n      const currentUser = auth.currentUser;\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          // title: Test,\n          // subtitle: this.timerange.content,\n          content: this.responseData,\n        })\n          .then(() => {\n            console.log(\"Card added to Firestore!\");\n          })\n          .catch((error) => {\n            console.error(\"Error adding card to Firestore: \", error);\n          });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.card-wrapper {\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  height: 150px; /* Adjust the height to your desired value */\n}\n\n.card-container {\n  margin-right: 20px;\n  max-width: 600px;\n}\n.scrollable {\n  max-height: 200px;\n  overflow: auto;\n}\n.card {\n  max-width: 600px;\n  height: 600px;\n  opacity: transparent\n}\n\n.card-title {\n  font-size: 1.5rem;\n  \n  color: white;\n}\n\n.card-subtitle {\n  font-size: 1.2rem;\n  color: white;\n}\n\n.card-text {\n  font-size: 1.2rem;\n  max-height: 600px;\n  overflow: hidden;\n}\n</style>\n\n\n"]},"metadata":{},"sourceType":"module"}