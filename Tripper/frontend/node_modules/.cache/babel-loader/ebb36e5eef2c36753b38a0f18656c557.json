ards\")\n          .add({\n            uId: userId,\n            title: card.title,\n            subtitle: card.subtitle,\n            content: card.content,\n          })\n          .then(() => {\n            console.log(\"Card added to Firestore!\");\n          })\n          .catch((error) => {\n            console.error(\"Error adding card to Firestore: \", error);\n          });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.card-wrapper {\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n}\n\n.card-container {\n  margin-right: 20px;\n  max-width: 400px;\n}\n\n.card {\n  max-width: 400px;\n  height: 400px;\n}\n\n.card-title {\n  font-size: 1.5rem;\n  color: white;\n}\n\n.card-subtitle {\n  font-size: 1.2rem;\n  color: white;\n}\n\n.card-text {\n  font-size: 1.2rem;\n  max-height: 100px;\n  overflow: hidden;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}