{"ast":null,"code":"export default {\n  data() {\n    return {\n      cards: [{\n        title: \"Card 1\",\n        subtitle: \"Subtitle 1\",\n        content: \"Content 1\"\n      }, {\n        title: \"Card 2\",\n        subtitle: \"Subtitle 2\",\n        content: \"Content 2\"\n      }, {\n        title: \"Card 3\",\n        subtitle: \"Subtitle 3\",\n        content: \"Content 3\"\n      }]\n    };\n  },\n\n  methods: {\n    saveToFirestore(card) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        collection(db, \"users\").doc(userId).collection(\"cards\").add({\n          uId: userId,\n          title: card.title,\n          subtitle: card.subtitle,\n          content: card.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CADK,EAML;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CANK,EAWL;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CAXK;IADF,CAAP;EAmBD,CArBY;;EAsBbC,OAAO,EAAE;IACPC,eAAe,CAACC,IAAD,EAAO;MACpB,MAAMC,WAAU,GAAIC,IAAI,CAACD,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAME,MAAK,GAAIF,WAAW,CAACG,GAA3B;QACAC,UAAU,CAACC,EAAD,EAAK,OAAL,CAAV,CACGC,GADH,CACOJ,MADP,EAEGE,UAFH,CAEc,OAFd,EAGGG,GAHH,CAGO;UACHC,GAAG,EAAEN,MADF;UAEHR,KAAK,EAAEK,IAAI,CAACL,KAFT;UAGHC,QAAQ,EAAEI,IAAI,CAACJ,QAHZ;UAIHC,OAAO,EAAEG,IAAI,CAACH;QAJX,CAHP,EASGa,IATH,CASQ,MAAM;UACVC,OAAO,CAACC,GAAR,CAAY,0BAAZ;QACD,CAXH,EAYGC,KAZH,CAYUC,KAAD,IAAW;UAChBH,OAAO,CAACG,KAAR,CAAc,kCAAd,EAAkDA,KAAlD;QACD,CAdH;MAeF,CAjBA,MAiBO;QACLH,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACF;IACD;;EAvBM;AAtBI,CAAf","names":["data","