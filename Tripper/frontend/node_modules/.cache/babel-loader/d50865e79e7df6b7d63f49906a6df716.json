{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection } from \"firebase/firestore\";\nexport default {\n  data() {\n    return {\n      cards: [{\n        title: \"Card 1\",\n        subtitle: \"Subtitle 1\",\n        content: \"Content 1\"\n      }, {\n        title: \"Card 2\",\n        subtitle: \"Subtitle 2\",\n        content: \"Content 2\"\n      }, {\n        title: \"Card 3\",\n        subtitle: \"Subtitle 3\",\n        content: \"Content 3\"\n      }]\n    };\n  },\n\n  methods: {\n    saveToFirestore(card) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        collection(db, \"users\").doc(userId).collection(\"cards\").add({\n          uId: userId,\n          title: card.title,\n          subtitle: card.subtitle,\n          content: card.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAeA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,QAA2B,oBAA3B;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CADK,EAML;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CANK,EAWL;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CAXK;IADF,CAAP;EAmBD,CArBY;;EAsBbC,OAAO,EAAE;IACPC,eAAe,CAACC,IAAD,EAAO;MACpB,MAAMC,WAAU,GAAIX,IAAI,CAACW,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAX,UAAU,CAACD,EAAD,EAAK,OAAL,CAAV,CACGa,GADH,CACOF,MADP,EAEGV,UAFH,CAEc,OAFd,EAGGa,GAHH,CAGO;UACHC,GAAG,EAAEJ,MADF;UAEHP,KAAK,EAAEK,IAAI,CAACL,KAFT;UAGHC,QAAQ,EAAEI,IAAI,CAACJ,QAHZ;UAIHC,OAAO,EAAEG,IAAI,CAACH;QAJX,CAHP,EASGU,IATH,CASQ,MAAM;UACVC,OAAO,CAACC,GAAR,CAAY,0BAAZ;QACD,CAXH,EAYGC,KAZH,CAYUC,KAAD,IAAW;UAChBH,OAAO,CAACG,KAAR,CAAc,kCAAd,EAAkDA,KAAlD;QACD,CAdH;MAeF,CAjBA,MAiBO;QACLH,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACF;IACD;;EAvBM;AAtBI,CAAf","names":["auth","db","collection","data","cards","title","subtitle","content","methods","saveToFirestore","card","currentUser","userId","uid","doc","add","uId","then","console","log","catch","error"],"sourceRoot":"","sources":["/Users/jasonti/Desktop/vue-firebase-auth-main/src/views/PlanView.vue"],"sourcesContent":["<template>\n  <div class=\"card-wrapper\">\n    <div v-for=\"(card, index) in cards\" :key=\"index\" class=\"card-container\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h5 class=\"card-title\">{{ card.title }}</h5>\n          <h6 class=\"card-subtitle mb-2\">{{ card.subtitle }}</h6>\n          <p class=\"card-text\">{{ card.content }}</p>\n          <button @click=\"saveToFirestore(card)\">Save</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { auth, db } from \"../main.js\";\nimport { collection } from \"firebase/firestore\";\n\nexport default {\n  data() {\n    return {\n      cards: [\n        {\n          title: \"Card 1\",\n          subtitle: \"Subtitle 1\",\n          content: \"Content 1\",\n        },\n        {\n          title: \"Card 2\",\n          subtitle: \"Subtitle 2\",\n          content: \"Content 2\",\n        },\n        {\n          title: \"Card 3\",\n          subtitle: \"Subtitle 3\",\n          content: \"Content 3\",\n        },\n      ],\n    };\n  },\n  methods: {\n    saveToFirestore(card) {\n      const currentUser = auth.currentUser;\n      if (currentUser) {\n        const userId = currentUser.uid;\n        collection(db, \"users\")\n          .doc(userId)\n          .collection(\"cards\")\n          .add({\n            uId: userId,\n            title: card.title,\n            subtitle: card.subtitle,\n            content: card.content,\n          })\n          .then(() => {\n            console.log(\"Card added to Firestore!\");\n          })\n          .catch((error) => {\n            console.error(\"Error adding card to Firestore: \", error);\n          });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}