{"ast":null,"code":"import { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  data() {\n    return {\n      response: null,\n      destination: null,\n      timerange: null\n    };\n  },\n\n  mounted() {\n    console.log(\"Query:\", this.$route.query);\n    const result = JSON.parse(this.$route.query.result);\n    console.log(\"Result:\", result);\n    this.destination = result.destination;\n    this.timerange = result.rangeTimePicker;\n    console.log(\"Destination:\", this.destination);\n    console.log(\"Timerange:\", this.timerange);\n    this.response = JSON.parse(this.$route.query.response);\n  },\n\n  methods: {\n    saveToFirestore(response) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        console.log(\"Result: \", this.result);\n        addDoc(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          title: this.destination.content,\n          subtitle: this.timerange.content,\n          content: response.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,WAAW,EAAE,IAFR;MAGLC,SAAS,EAAE;IAHN,CAAP;EAKD,CAPY;;EAQbC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,QAAZ,EAAsB,KAAKC,MAAL,CAAYC,KAAlC;IACA,MAAMC,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAW,KAAKJ,MAAL,CAAYC,KAAZ,CAAkBC,MAA7B,CAAf;IACAJ,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBG,MAAvB;IACA,KAAKP,WAAL,GAAmBO,MAAM,CAACP,WAA1B;IACA,KAAKC,SAAL,GAAiBM,MAAM,CAACG,eAAxB;IACAP,OAAO,CAACC,GAAR,CAAY,cAAZ,EAA4B,KAAKJ,WAAjC;IACAG,OAAO