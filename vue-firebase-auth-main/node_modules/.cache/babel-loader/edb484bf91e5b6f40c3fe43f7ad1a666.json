{"ast":null,"code":"// Import the auth and db objects from your main.js file\nimport { auth, db } from \"../main.js\";\nimport { collection, doc, addDoc } from \"firebase/firestore\";\nexport default {\n  data() {\n    return {\n      cards: [{\n        title: \"Card 1\",\n        subtitle: \"Subtitle 1\",\n        content: \"Content 1\"\n      }, {\n        title: \"Card 2\",\n        subtitle: \"Subtitle 2\",\n        content: \"Content 2\"\n      }, {\n        title: \"Card 3\",\n        subtitle: \"Subtitle 3\",\n        content: \"Content 3\"\n      }]\n    };\n  },\n\n  methods: {\n    saveToFirestore(card) {\n      const currentUser = auth.currentUser;\n\n      if (currentUser) {\n        const userId = currentUser.uid;\n        add(collection(doc(collection(db, \"plan\"), userId), \"cards\"), {\n          uId: userId,\n          title: card.title,\n          subtitle: card.subtitle,\n          content: card.content\n        }).then(() => {\n          console.log(\"Card added to Firestore!\");\n        }).catch(error => {\n          console.error(\"Error adding card to Firestore: \", error);\n        });\n      } else {\n        console.log(\"No user currently logged in.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA;AACA,SAASA,IAAT,EAAeC,EAAf,QAAyB,YAAzB;AACA,SAASC,UAAT,EAAqBC,GAArB,EAA0BC,MAA1B,QAAwC,oBAAxC;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CADK,EAML;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CANK,EAWL;QACEF,KAAK,EAAE,QADT;QAEEC,QAAQ,EAAE,YAFZ;QAGEC,OAAO,EAAE;MAHX,CAXK;IADF,CAAP;EAmBD,CArBY;;EAsBbC,OAAO,EAAE;IACPC,eAAe,CAACC,IAAD,EAAO;MACpB,MAAMC,WAAU,GAAIb,IAAI,CAACa,WAAzB;;MACA,IAAIA,WAAJ,EAAiB;QACf,MAAMC,MAAK,GAAID,WAAW,CAACE,GAA3B;QACAC,GAAG,CAACd,UAAU,CAACC,GAAG,CAACD,UAAU,CAACD,EAAD,EAAK,MAAL,CAAX,EAAyBa,MAAzB,CAAJ,EAAsC,OAAtC,CAAX,EACH;UACEG,GAAG,EAAEH,MADP;UAEEP,KAAK,EAAEK,IAAI,CAACL,KAFd;UAGEC,QAAQ,EAAEI,IAAI,CAACJ,QAHjB;U